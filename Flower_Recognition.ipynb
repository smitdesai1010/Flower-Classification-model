        "def create_dataset(dir):\n",
        "  path='flowers/'+dir+'/'\n",
        "\n",
        "  for file in  os.listdir(path):\n",
        "\n",
        "    img=cv2.imread(path+file , cv2.IMREAD_COLOR)\n",
        "\n",
        "    try:\n",
        "     img=cv2.resize(img, (250,250))\n",
        "    \n",
        "    except:\n",
        "     continue \n",
        "\n",
        "    images.append(np.array(img))     \n",
        "    label.append(dir)"
